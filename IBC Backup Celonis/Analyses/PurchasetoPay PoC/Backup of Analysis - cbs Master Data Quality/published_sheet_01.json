            "expressionLhs": "CASE WHEN ( CASE WHEN PROCESS # 'Erfasse Wareneingang' THEN CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END ELSE CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END END ) / \"EKPO\".\"PLIFZ\" < 0.5 THEN 'e) Ratio < 50%' WHEN ( CASE WHEN PROCESS # 'Erfasse Wareneingang' THEN CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END ELSE CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END END ) / \"EKPO\".\"PLIFZ\" BETWEEN 0.5 AND 0.8 THEN 'd) Ratio between 50% and 80%' WHEN ( CASE WHEN PROCESS # 'Erfasse Wareneingang' THEN CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END ELSE CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END END ) / \"EKPO\".\"PLIFZ\" BETWEEN 0.8 AND 1.2 THEN 'c) Ratio between 80% and 120%' WHEN ( CASE WHEN PROCESS # 'Erfasse Wareneingang' THEN CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Wareneingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END ELSE CASE WHEN PROCESS # 'Sende Bestellung' THEN CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Sende Bestellung' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) ELSE CALC_THROUGHPUT ( FIRST_OCCURRENCE [ 'Lege Bestellposition an' ] TO LAST_OCCURRENCE [ 'Erfasse Rechnungseingang' ] , REMAP_TIMESTAMPS ( \"_CEL_P2P_ACTIVITIES\".\"EVENTTIME\" , DAYS ) ) END END ) / \"EKPO\".\"PLIFZ\" BETWEEN 1.2 AND 1.5 THEN 'b) Ratio between 120% and 150%' ELSE 'a) Ratio > 150%' END AS \"Ratio Category\"",
